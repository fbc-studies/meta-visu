{
  "env": {
    "browser": true
  },
  "globals": {
    "d3": false,
    "TreeChart": false,
    "CategoryTimeline": false
  },
  "extends": "airbnb-base",
  "rules": {
    "_comment": "disables no-param-reassign for parameter's properties",
    "no-param-reassign": [2, { "props": false }],
    "no-mixed-operators": "off",
    "no-use-before-define": "off"
  }
}
